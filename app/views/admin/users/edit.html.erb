<h1>ユーザー編集</h1>
<br>
<%= form_with(model: @user ,local: true, url: admin_user_path) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br><br>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <br><br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br><br>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <br><br>
  <%= f.submit "Create account" %>
  <br><br>
<% end %>